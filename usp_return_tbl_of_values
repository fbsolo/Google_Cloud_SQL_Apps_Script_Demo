USE DEMO;

DELIMITER $$

DROP PROCEDURE IF EXISTS usp_return_tbl_of_values$$
CREATE PROCEDURE usp_return_tbl_of_values(IN input_param INTEGER)
BEGIN
	DECLARE PARSE_STRING VARCHAR(1000);
	SET @PARSE_STRING = NULL;
	CALL usp_build_comma_dlm_string(input_param, @PARSE_STRING);

	SELECT * FROM EXAMPLE_TABLE
	WHERE FIND_IN_SET(EXAMPLE_TABLE.ROW_ID, @PARSE_STRING)
	ORDER BY ROW_ID;

END$$
DELIMITER ;
